{
  "name": "prato",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "sass": "node-sass sass/main.scss public/css/style.css -w",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js ",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "server": "nodemon server.js -e ejs,js",
    "server:prod": "NODE_ENV=production nodemon server.js -e ejs,js",
    "start:dev": "npm-run-all --parallel server sass watch:js",
    "start:prod": "npm-run-all --parallel server:prod sass build:js",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lcolsant/prato.git"
  },
  "author": "Lee Colsant",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lcolsant/prato/issues"
  },
  "homepage": "https://github.com/lcolsant/prato#readme",
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "aws-sdk": "^2.847.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "drag-drop-touch": "^1.3.1",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "helmet": "^4.4.1",
    "html-to-text": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.23",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.16",
    "sharp": "^0.22.1",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "livereload": "^0.9.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4"
  },
  "engines": {
    "node": "^10"
  }
}
